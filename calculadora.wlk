/** Ejemplo Calculadora */
object calculadora{
	
		var numero1=20
		var numero2=40
	
	method  CuentaSuma() {	
		
	return numero1+numero2
		
		}
	method  CuentaResta() {	
		
	return numero1-numero2
		
		}
	method  CuentaMultiplicacion() {	
		
	return numero1*numero2
		
		}
	method  CuentaDivision() {	
		
	return numero1/numero2
		
		}
}
